<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="Softhand.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:converters="clr-namespace:Softhand.Infrastructure.Converters"
    xmlns:local="clr-namespace:Softhand"
    xmlns:m="clr-namespace:UraniumUI.Material.Resources;assembly=UraniumUI.Material">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                <m:StyleResource />
            </ResourceDictionary.MergedDictionaries>
            <converters:SipStatusColorConverter x:Key="SipStatusColorConverter" />
        </ResourceDictionary>
    </Application.Resources>

</Application>

